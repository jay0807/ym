body{-moz-user-select:none;cursor:pointer;background-color:#fff;}.drag_operate_box{border:1px solid #36c;font-size:12px;font-family:Arial,Helvetica,sans-serif;position:absolute;color:#fff!important;z-index:65535;box-shadow:0 3px 7px #333;-moz-box-shadow:0 3px 7px #333;-webkit-box-shadow:0 3px 7px #333;}.drag_operate_box_moving{background-color:#e9e9e9;border-style:solid;}.drag_handle_box{line-height:24px;height:24px;overflow:hidden;background:#5983D6;position:relative;white-space:nowrap;}.drag_handle_box span{display:block;}.drag_handle_box .dhb_title{text-align:left;padding:0 5px 0 5px;cursor:move;}.drag_handle_box .dhb_edit,.drag_handle_box .dhb_del{cursor:pointer;}.drag_handle_box table td{text-align:center;vertical-align:middle;}.drag_ghost_box{border:1px solid #F00;background:#F30;opacity:.5;filter:alpha(opacity=50);position:absolute;z-Index:65534;box-shadow:0 3px 7px #333;-moz-box-shadow:0 3px 7px #333;-webkit-box-shadow:0 3px 7px #333;}.drop_nochild{line-height:25px;text-align:center;color:#ccc;border:1px #006 dotted;}.widget_resize{height:5px;background:#ccc;border-top:1px #999 solid;cursor:row-resize;}.widgets_drop_ghost{position:absolute;z-Index:65535;border:1px dotted #f60;background:#06C;color:#000;}.widgets_drop_ghost_on{background:#6cc;border:1px solid #333;}.empty_drop_box{line-height:20px;border:1px #006 dotted;background:#e9e9e9;}.sWidgets_flash{border:1px #ccc solid;background:#f1f1f1 url(statics/weiget_flash.gif) no-repeat center center;}