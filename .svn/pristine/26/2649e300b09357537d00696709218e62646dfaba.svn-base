body,h1,h2,h3,h4,h5,h6,hr,p,blockquote,dl,dt,dd,ul,ol,li,pre,form,fieldset,legend,button,input,textarea,th,td{margin:0;padding:0;}
body,button,input,select,textarea{font:12px/1.5 SimSun tahoma,arial,\5b8b\4f53;}
h1,h2,h3,h4,h5,h6{font-size:100%;}
address,cite,dfn,em,var{font-style:normal;}
code,kbd,pre,samp{font-family:courier new,courier,monospace;}
small{font-size:12px;}
ul,ol{list-style:none;}
a{text-decoration:none;}
a:hover{text-decoration:underline;}
sup{vertical-align:text-top;}
sub{vertical-align:text-bottom;}
legend{color:#000;}
fieldset,img{border:0;}
button,input,select,textarea{font-size:100%;}
table{border-collapse:collapse;border-spacing:0;}
.clearfix:before,.clearfix:after {content: '.';display: block;overflow: hidden;visibility: hidden;font-size: 0;line-height: 0;width: 0;height: 0;}
.clearfix:after {clear: both;}

#pager-wrapper, #header, #body, #footer {width:100%;}
.position {width:1000px; margin:0 auto;}
#body {padding:16px 0 29px; background:#e7eeee;}
#header {height:114px; background:url(hdbg1.jpg) repeat-x 0 bottom;}
#header .brand {display:inline; float:left; width:189px; height:63px; margin:24px 21px 0 0;}
#header .brand h2 {background:url(logo1.jpg) no-repeat;}
#header .brand h2, #header .brand a {display:inline-block; width:100%; height:100%;}
#header .brand span {margin-left:-9999em;}
#header .user-panel {float:left; width:160px; margin:46px 0 0; color:#999;}
#header .user-panel a {display:block;}
.site-nav {float:right; margin:43px 0 0;}
.site-nav li {float:left;}
.site-nav li a {float:left; height:40px; text-indent:-9999em; background:url(sprites1.jpg) no-repeat;}
.site-nav .dt a {width:80px; background-position:0 0;}
.site-nav .mc a {width:136px; background-position:-80px 0;}
.site-nav .uc a {width:156px; background-position:-216px 0;}
.site-nav .pd a {width:118px; background-position:-372px 0;}
.site-nav .dt.active a, .site-nav .dt a:hover {background-position:0 -40px;}
.site-nav .mc.active a, .site-nav .mc a:hover {background-position:-80px -40px;}
.site-nav .uc.active a, .site-nav .uc a:hover {background-position:-216px -40px;}
.site-nav .pd.active a, .site-nav .pd a:hover {background-position:-372px -40px;}
#footer {height:70px; background:#333;}
#footer p {text-align:right; line-height:65px; color:#717171;}

.main {float:left; width:798px;}
.home .main {width:758px;}
.asider {float:left; width:200px; overflow:hidden;}
.lt, .rt {background:#fdfdfd url(sd1.jpg) repeat-y;}
.lt {background-position:0 0;}
.rt {background-position:-9px 0;}
#body .splitlt {background:#fdfdfd url(sd2.jpg) repeat-y 192px 0 !important;}
#body .splitrt {background:#fdfdfd url(sd1.jpg) repeat-y 799px 0 !important;}
.apex {margin:-22px 0 0;}
.btn-join, .btn-join:visited, .btn-save, .btn-save:visited {display:inline-block; width:160px; height:41px; border:0; cursor:pointer; text-indent:-9999em; background:url(../images/sprites1.jpg) no-repeat;}
.ie6 .btn-join, .ie6 .btn-join:visited, .ie6 .btn-save, .ie6 .btn-save:visited {display:block;}
.btn-join, .btn-join:visited {background-position:0 -100px;}
.btn-join:hover {background-position:-170px -100px;}
.btn-save, .btn-join:visited {background-position:0 -151px;}
.btn-save:hover {background-position:-170px -151px;}

#body .position {width:998px; padding-bottom:32px; border:1px solid #e3e9e9; border-radius:5px; -moz-border-radius:5px; -webkit-border-radius:5px; -o-border-radius:5px; background:#fff;}
.home .position {width:960px !important; padding:22px 19px 32px !important;}
.wlist {clear:both; margin:0 39px; padding:20px 0 0; font-size:0;}
.wlist .item {display:inline; float:left; width:200px; margin:0 15px; font-size:12px;}
.ie6 .wlist .item {float:left;}
.wlist .header, .article h3, .enable-shop .header {width:100%; height:36px; overflow:hidden; border-bottom:1px solid #5bab9b; line-height:40px;}
.wlist .header strong, .article h3 span, .enable-shop .header strong {float:left; height:31px; padding:0 66px 0 0; border-bottom:5px solid #5bab9b; font-size:13px; color:#5bab9b;}
.ie6 .wlist .header strong, .ie6 .article h3 span, .ie6 .enable-shop .header strong {overflow:hidden; position:relative;}
.wlist .header a, .wlist .header a:visited {float:right; margin:12px 0 0; font-size:12px; line-height:16px; color:#5bab9b;}

.wlist .item a {display:block; width:178px; height:142px; margin:12px auto; position:relative; border:1px solid #ddd;}
.wlist .item img {border:0;}
.wlist .item a span, .wlist .item a:visited span {display:none; width:180px; height:25px; position:absolute; left:-1px; bottom:-1px; color:#fff; text-align:center; line-height:25px; background:#666;}
.wlist .item a, .wlist .item a:visited {text-decoration:none;}
.wlist .item a:hover span, .wlist .item a.active span {display:block !important;}

.tips {padding:10px 0; border:1px solid #ffcc66; text-align:center; line-height:20px; color:#996600; background:#ffefb1;}
.tips a, .tips a:visited {text-decoration:underline; color:#ff6600;}
.tips a:hover {text-decoration:none;}
.col-5 {margin:0 0 20px; padding-top:0;}
.col-5 .item {width:180px; margin:0 15px 0 0;}
.col-5 .last {margin:0;}

.section {margin:15px 0 0;}
.hero { height:208px; border:1px solid #bfbfbf; background:#e0e0e0;}
.asider .list {margin:0 2px 0 15px;}
.asider .list .header, .asider .plist .header {color:#479989;}
.asider .list .header h5, .asider .plist .header h5 {text-align:center; line-height:36px;}
.pen, .buglet, .ico-cart, .ico-sina, .ico-weixin, .tip i, .err i, .pass i {background:url(icons1.gif) no-repeat}
.buglet {background-position:50px -166px;}
.pen {background-position:42px -196px;}

.asider .list ul, .asider .plist ul {border-top:1px dashed #ededed;}
.asider .list li {float:left; width:100%; height:27px; border-bottom:1px dashed #ededed; line-height:27px;}
.asider .list a, .asider .list a:visited {float:left; width:100%; height:100%; color:#999; text-indent:18px; background:url(icons1.gif) no-repeat 7px -228px;}
.asider .list a:hover {color:#479989; background-position:7px -258px;}

.article {padding:22px 39px 0;}
.article p {margin:10px 0; font-size:13px; line-height:24px; color:#666;}
.rtop {display:inline; width:180px; margin:0 9px 0 11px;}
.asider .plist ul {padding:8px 0 0;}
.asider .plist img {border:1px solid #ddd;}
.asider .plist li {margin:0 0 5px;}
.rtop .list {margin:0;}

.splist {padding:0 9px 0 8px; color:#888;}
.splist li {float:left; width:100%; height:41px; position:relative; border-bottom:1px dashed #ededed; font-size:13px; text-align:center; line-height:41px;}
.splist li a, .splist li a:visited {color:#888;}
.splist li a:hover, .splist li.active a, .ie6 .splist li.active a.ico-cart {color:#5bab9b;}
.splist li.active a {font-weight:700; }
.splist li a, .splist li span {padding:0 0 0 20px;}
.splist .active i {display:block; width:8px; height:13px; position:absolute; top:15px; right:-11px; background:url(icons1.gif) no-repeat 0 -300px;}

.ico-cart {background-position:0 -90px;}
.ico-sina {background-position:0 -119px;}
.ico-weixin {background-position:0 -148px;}
.ico-soon {padding:0 55px 17px 0 !important; background:url(sprites1.jpg) no-repeat -340px -100px;}
.ie6 .splist li .ico-soon, .ie7 .splist li .ico-soon {zoom:1; position:relative; top:5px; font-size:0;}
.ie6 .splist li .ico-soon {margin:6px 0 0;}
.enable-shop .asider {overflow:visible;}
.enable-shop .main {width:720px; padding:13px 39px;}
.enable-shop form {width:100%; overflow:hidden;}
.enable-shop .tips {border-color:#c5dbe3; color:#666; background:#ebf5f9;}
.enable-shop .tips strong {font-size:14px; font-weight:400; color:#cc9900; text-decoration:underline;}
.enable-shop .tips a, .enable-shop .tips a:visited {color:#b7b7b7;}
.enable-shop .header {margin:0 0 20px;}
.enable-shop li {float:left; width:100%;}
.enable-shop .text-wrap {padding:0 0 10px; line-height:38px;}
.enable-shop .text-wrap div {float:left;}
.enable-shop .text-wrap label {float:left; width:122px;}
.enable-shop .text-wrap input {float:left; width:252px; height:18px; padding:10px; border:0; line-height:18px; background:url(../images/sprites1.jpg) no-repeat 0 -200px;}
.enable-shop .text-wrap input:focus {outline:0;}
.enable-shop .action {padding:20px 0 0 122px;}
.msgbox {padding:11px 0 12px 15px; line-height:15px;}
.msgbox i {display:inline; float:left; width:16px; height:15px; margin:0 10px 0 0;}
.tip i {background-position:0 -60px;}
.err i {background-position:0 -30px;}
.pass i {background-position:0 0;}
